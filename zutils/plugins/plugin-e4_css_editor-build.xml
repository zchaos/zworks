<?xml version="1.0" encoding="UTF-8"?>
<project name="project" default="download_eclipse_plugins">
	<target name="download_eclipse_plugins">
		<property name="e4_css_editor" value="/zss/env/eclipse/plugins/e4_css_editor" />
		<p2.mirror destination="file:/${e4_css_editor}" description="e4_css_editor" verbose="true">
			<source>
				<repository name="e4_css_editor" location="http://download.eclipse.org/e4/updates/0.15-I-builds/" />
			</source>
			<slicingOptions includeFeatures="true" followStrict="true" latestVersionOnly="true" />
		</p2.mirror>
	</target>
</project>